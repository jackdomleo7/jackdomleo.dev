{
  "name": "jackdomleo.dev",
  "version": "1.0.0",
  "private": true,
  "description": "Jack Domleo's personal website and blog",
  "author": {
    "name": "Jack Domleo",
    "url": "https://jackdomleo.dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackdomleo7/jackdomleo.dev.git"
  },
  "homepage": "https://jackdomleo.dev",
  "scripts": {
    "deps": "rimraf ./node_modules && npm ci",
    "dev": "nuxt --open",
    "start": "nuxt start --open",
    "generate": "rimraf ./dist && nuxt generate -m",
    "lint:check": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore --max-warnings=0 .",
    "lint:fix": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore --fix --cache --max-warnings=0 .",
    "test": "rimraf ./coverage && jest",
    "test:update-snapshots": "rimraf ./coverage && jest --updateSnapshot",
    "deploy": "npm run generate && push-dir --dir=dist --branch=gh-pages --cleanup"
  },
  "dependencies": {
    "@nuxt/content": "^1.15.1",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/pwa": "^3.3.5",
    "@vue/cli-plugin-unit-jest": "^4.5.15",
    "core-js": "^3.20.2",
    "date-fns": "^2.28.0",
    "modern-normalize": "^1.1.0",
    "nuxt": "^2.15.8",
    "vuelidate": "^0.7.7"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.5",
    "@nuxt/image": "^0.6.0",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/color-mode": "^2.1.1",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/eslint-config-typescript": "^8.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/google-analytics": "^2.4.0",
    "@nuxtjs/prismic": "^1.4.0",
    "@nuxtjs/robots": "^2.5.0",
    "@nuxtjs/sitemap": "^2.4.0",
    "@nuxtjs/style-resources": "^1.2.1",
    "@nuxtjs/svg-sprite": "^0.5.2",
    "@types/jest": "^26.0.24",
    "@types/jest-axe": "^3.5.3",
    "@types/vuelidate": "^0.7.15",
    "@vue/test-utils": "^1.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^26.6.3",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^3.1.0",
    "eslint-plugin-vue": "^8.2.0",
    "jest": "^26.6.3",
    "jest-axe": "^5.0.1",
    "prettier": "^2.5.1",
    "push-dir": "^0.4.1",
    "rimraf": "^3.0.2",
    "sass": "^1.45.2",
    "sass-loader": "^10.2.0",
    "ts-jest": "^26.5.6",
    "vue-jest": "^3.0.7"
  }
}
